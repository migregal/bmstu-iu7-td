# lint =========================================================================

.PHONY: b-lint
b-lint:
	$(MAKE) -C ./backend lint

# lint ^========================================================================

# unit =========================================================================

.PHONY: b-unit-test
b-unit-test:
	$(MAKE) -C ./backend unit-test

# unit ^========================================================================

# build ========================================================================

.PHONY: b-build
b-build:
	$(MAKE) -C ./backend build

# build ^=======================================================================

# dev ==========================================================================

.PHONY: dev-start
dev-start:
	docker compose -f ./docker/docker-compose.dev.yml up --build

# dev ^=========================================================================
